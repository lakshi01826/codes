package com.java;

public class Array {
	public static void main(String[]  args) {
		
		       
		        int[] num = {10, 15, 22, 33, 40, 55, 68, 118, 23, 50};
		        
		       
		        for (int i = 0; i < num.length; i++) {
		            
		            
		            if (num[i] % 2 == 0) {
		                System.out.println(num[i] + " is Even");
		            }
		        }
		    }

}
