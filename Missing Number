package com.java;

public class MissingNumber {

    public static void main(String[] args) {
        int[] arr = {1, 2, 3, 4, 5, 6, 7, 9, 8};
        int n = arr.length + 1; // Total numbers should be 10 here
        int totalSum = n * (n + 1) / 2; // Formula for sum of 1 to n

        int sum = 0;
        for (int num : arr) {
            sum += num;
        }

        System.out.println("Missing number: " + (totalSum - sum));
    }
}


