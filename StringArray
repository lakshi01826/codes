package com.java;

public class StringArray {
	public static void main(String[] args) {
		String[] [] arr= {{"cat","leg","pen","dog"},
				          {"sun","hat","run","bed"},
				          {"map","cup","bat","box"},
				          {"car","hen","fan","top"}
		                };
		int count = 1;
		
		for(int i = 0; i<arr.length; i++) {
			for(int j = 0; j<arr.length; j++) {
				System.out.println(count + "." + arr[i][j]);
				count++;
			
				
			
		}
		}
	}
}
	
